  if (hasError) {\n    return <Alert message=\"Error\" description=\"Couldn't find the article\" type=\"error\" showIcon />;\n  }\n\n  if (isLoading) {\n    return <Spin className={cls.Spin} size=\"large\" tip=\"Loading...\" />;\n  }\n\n  return (\n    <div className={classes.NewArticle}>\n      <h2 className={classes.NewArticle__Title}>Edit Article</h2>\n      {item && <NewArticleForm onSubmitArticle={submitArticle} article={item} />}\n      <TagList tags={tags} onDeleteTag={(tag) => deleteTag(tags, tag, setTags)} />\n      <TagForm onAddTag={(tag) => addTag(tags, tag, setTags)} />\n    </div>\n  );\n};\n\nexport default EditArticle;\n","D:\\Web\\Polina\\blog\\src\\utils\\addTagDeleteTag.js",[],{"ruleId":"129","replacedBy":"130"},{"ruleId":"131","replacedBy":"132"},{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","replacedBy":"136"},{"ruleId":"137","replacedBy":"138"},{"ruleId":"139","severity":1,"message":"140","line":33,"column":25,"nodeType":"141","messageId":"142","endLine":33,"endColumn":36},{"ruleId":"139","severity":1,"message":"140","line":20,"column":25,"nodeType":"141","messageId":"142","endLine":20,"endColumn":36},{"ruleId":"139","severity":1,"message":"140","line":41,"column":23,"nodeType":"141","messageId":"142","endLine":41,"endColumn":34},{"ruleId":"139","severity":1,"message":"140","line":43,"column":23,"nodeType":"141","messageId":"142","endLine":43,"endColumn":34},{"ruleId":"139","severity":1,"message":"140","line":42,"column":23,"nodeType":"141","messageId":"142","endLine":42,"endColumn":34},{"ruleId":"139","severity":1,"message":"140","line":29,"column":23,"nodeType":"141","messageId":"142","endLine":29,"endColumn":34},{"ruleId":"139","severity":1,"message":"140","line":50,"column":23,"nodeType":"141","messageId":"142","endLine":50,"endColumn":34},"lines-around-directive",["143"],"global-require",[],"no-buffer-constructor",[],"no-new-require",[],"no-path-concat",[],"no-console","Unexpected console statement.","MemberExpression","unexpected","